<div class="content-page">
  <div class="content">
    <div class="content-wrapper">
      <!-- Main content -->
      <section class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <!-- Customer List -->
              <div class="card card-primary card-outline">
                <div class="card-header">
                  <h5 class="card-title">Khách Hàng</h5>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                  <div class="row gap-3 justify-content-center">
                    <% listAccountUserData.accounts.forEach((row)=> { %>
                      <div class="col-md-5 mr-4">
                        <a href="/account/detail-user/<%= row._id %>" class="text-decoration-none text-black">
                        <div class="row border rounded rounded-lg pt-3 pb-3 mb-3">
                          <div class="col-md-3">
                            <img src="<%= row.avatarUrl %>" alt="Product Image"
                              style="width: 100px; height: 100px; border-radius: 10px;">
                          </div>
                          <div class="col-md-9">
                            <h5 class="font-weight-bold">Tên: <%= row.userName %>
                            </h5>
                            <p class="font-weight-normal">Email: <%= row.email %>
                            </p>
                            <p class="font-weight-normal">So dien thoai: <%= row.phoneNumber %>
                            </p>
                          </div>
                        </div>
                      </a>
                      </div>
                      <% }) %>
                  </div>
                </div>
                <!-- /.card-body -->
              </div>
              <!-- /.card -->
            </div>
          </div>
        </div>
      </section>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
  </div>
</div>
<!-- ./wrapper -->
</div>