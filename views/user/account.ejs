<div class="content-page">
  <div class="content">
    <div class="content-wrapper">
      <!-- Main content -->
      <section class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <!-- Customer List -->
              <div class="card card-primary card-outline">
                <div class="card-header">
                  <h5 class="card-title">Khách Hàng</h5>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                  <table class="table table-hover">
                    <thead>
                      <tr>
                        <th>STT</th>
                        <th>Tên</th>
                        <th>Email</th>
                        <th>Số điện thoại</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% let index=1; %>
                        <% listAccountUserData.accounts.forEach((row)=> { %>
                          <tr>
                            <td>
                              <%= index %>
                            </td>
                            <td>
                              <a href="/account/detail-user/<%= row._id %>" class="text-decoration-none text-black">
                                <%= row.userName %>
                              </a>
                            </td>
                            <td>
                              <%= row.email %>
                            </td>
                            <td>
                              <%= row.phoneNumber %>
                            </td>
                          </tr>
                          <% index++; %>
                            <% }) %>
                    </tbody>
                  </table>
                </div>
                <!-- /.card-body -->
              </div>
              <!-- /.card -->
            </div>
          </div>
        </div>
      </section>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
  </div>
</div>
<!-- ./wrapper -->
</div>